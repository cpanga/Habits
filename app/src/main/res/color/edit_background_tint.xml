<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Disabled state -->
    <item android:alpha="@dimen/material_emphasis_disabled_background" android:color="@color/md_theme_light_secondary" android:state_enabled="false" />
    <!-- Checkable, unchecked state -->
    <item android:color="@color/md_theme_light_primaryContainer" android:state_checkable="true" android:state_checked="false" />
    <!-- Default, Enabled state -->
    <item android:color="@color/md_theme_light_primary" />
</selector>
